{
  "name": "cryptoloco",
  "version": "1.0.0",
  "description": "This repo contains the sourcode for cryptoloco dashboard",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "start": "node build/server.js",
    "start:dev": "nodemon --watch server --exec babel-node -- server/server.js",
    "dev": "webpack && webpack-dev-server --config webpack.config.js --progress --inline --colors",
    "watch": "webpack --config webpack.dev.js --watch",
    "build:prod": "webpack --config ./webpack.prod.js",
    "build:dev": "webpack --config ./webpack.dev.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./server -s",
    "build:server": "babel server",
    "build": "npm run clean && npm run build:prod && npm run build-server",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize --migrations-path=server/migrations --models-path=server/models --config=server/config/config.js db:seed:all",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boluwatifes/react-redux-boilerplate.git"
  },
